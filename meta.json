{
  "module_id": "viam-soleng:viam-raspi-sensors",
  "visibility": "public",
  "url": "https://github.com/viam-soleng/viam-raspi-sensors",
  "description": "A collection of sensors and utilities for the Rasperry Pi",
  "models": [
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:raspi:clocks"
    },
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:raspi:cpu_manager"
    },
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:raspi:voltages"
    },
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:raspi:temperatures"
    },
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:raspi:throttling"
    },
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:raspi:pwm_fan"
    }
  ],
  "build": {
    "build": "make package",
    "path": "bin/viam-raspi-sensors",
    "arch": ["linux/amd64", "linux/arm64"]
  },
  "entrypoint": "bin/viam-raspi-sensors",
  "first_run": ""
}
