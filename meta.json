{
  "module_id": "rinzlerlabs:viam-raspi-sensors",
  "visibility": "public",
  "url": "https://github.com/rinzlerlabs/viam-raspi-sensors",
  "description": "A collection of sensors and utilities for single board computers (SBC) like the Raspberry Pi or NVIDIA Jetson Nano.",
  "models": [
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:sbc:clocks"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:sbc:cpu_manager"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:sbc:voltages"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:sbc:temperatures"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:sbc:throttling"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:sbc:pwm_fan"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:sbc:cpu_monitor"
    }
  ],
  "build": {
    "build": "make package",
    "path": "bin/rinzlerlabs-sbc-sensors",
    "arch": ["linux/arm64"]
  },
  "entrypoint": "bin/rinzlerlabs-sbc-sensors",
  "first_run": ""
}
